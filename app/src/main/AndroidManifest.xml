<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lu_xinghe.project600final">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="SU News"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".newsPage.NewsPageActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".newsDetails.NewsDetailsActivity" />
        <activity
            android:name=".Favorites.FavoritesActivity"
            android:label="@string/title_activity_favorites"
            android:theme="@style/AppTheme"></activity>

        <activity
            android:name=".Favorites.FavDetails.FavDetailsActivity"
            android:label="@string/title_activity_favorites"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>
